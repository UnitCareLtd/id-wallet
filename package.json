{
    "name": "id-wallet",
    "version": "1.0.0",
    "description": "UnitCare Identity Wallet for UnitCare Identity Token",
    "main": "index.js",
    "repository": "https://github.com/UnitCare/id-wallet.git",
    "author": "UnitCare <support@unitcare.co>",
    "license": "MIT",
    "scripts": {
        "setup": "cp .env.example .env",
        "dev": "concurrently \"yarn run develop \" \"yarn run develop:webpack \" \"yarn run develop:blockchain \" ",
        "develop": "nodemon --ignore lib --exec babel-node src/server",
        "develop:webpack": "webpack-dev-server --progress",
        "develop:blockchain": "ganache-cli -m 'candy maple cake sugar pudding cream honey rich smooth crumble sweet treat'",
        "build": "rimraf lib && babel src -d lib --ignore .test.js && cross-env NODE_ENV=production webpack -p --progress",
        "start": "cross-env NODE_ENV=production pm2 start lib/server && pm2 logs",
        "stop": "pm2 delete server",
        "lint": "eslint src webpack.config.babel.js --ext .js, .jsx",
        "test": "yarn lint && jest --coverage",
        "compile": "truffle compile",
        "migrate": "truffle migrate"
    },
    "devDependencies": {
        "autoprefixer": "^8.2.0",
        "babel-cli": "^6.26.0",
        "babel-core": "^6.26.0",
        "babel-jest": "^21.2.0",
        "babel-loader": "^7.1.2",
        "babel-preset-env": "^1.6.1",
        "babel-preset-react": "^6.24.1",
        "bootstrap": "4.0.0",
        "concurrently": "^3.5.1",
        "cross-env": "^5.1.1",
        "css-loader": "^0.28.11",
        "eslint": "^4.13.1",
        "file-loader": "^1.1.11",
        "husky": "^0.14.3",
        "jest": "^21.2.1",
        "node-sass": "^4.8.3",
        "nodemon": "^1.13.3",
        "pm2": "^2.9.1",
        "postcss-loader": "^2.1.3",
        "rimraf": "^2.6.2",
        "sass-loader": "^6.0.7",
        "style-loader": "^0.20.3",
        "webpack": "^3.10.0",
        "webpack-dev-server": "^3.1.11"
    },
    "dependencies": {
        "@fortawesome/fontawesome": "^1.1.7",
        "@fortawesome/fontawesome-free-regular": "^5.0.12",
        "@fortawesome/fontawesome-free-solid": "^5.0.12",
        "@fortawesome/react-fontawesome": "^0.0.19",
        "babel-polyfill": "^6.26.0",
        "babel-register": "^6.26.0",
        "bignumber.js": "^6.0.0",
        "chai": "^4.1.2",
        "chai-bignumber": "^2.0.2",
        "compression": "^1.7.1",
        "dotenv": "^5.0.1",
        "express": "^4.16.2",
        "fast-csv": "^2.4.1",
        "ganache-cli": "^6.1.0",
        "isomorphic-fetch": "^2.2.1",
        "jquery": "^3.2.1",
        "jquery.facedetection": "^2.0.3",
        "popper.js": "^1.14.0",
        "prop-types": "^15.6.1",
        "qrcode-react": "^0.1.16",
        "react": "^16.2.0",
        "react-clmtrackr": "^1.1.3",
        "react-dom": "^16.2.0",
        "react-helmet": "^5.2.0",
        "react-hot-loader": "^3.0.0",
        "react-qr-reader": "^2.1.0",
        "react-redux": "^5.0.7",
        "react-router": "^4.2.0",
        "react-router-dom": "^4.2.2",
        "react-webcam": "^0.2.0",
        "redux": "^3.7.2",
        "redux-thunk": "^2.2.0",
        "tether": "^1.4.3",
        "truffle": "^4.1.5",
        "truffle-hdwallet-provider": "^0.0.3",
        "victory": "^0.25.6",
        "web3": "0.20.6",
        "web3-utils": "1.0.0-beta.33"
    },
    "jest": {
        "testPathIgnorePatterns": [
            "<rootDir>/test/"
        ]
    }
}